{
  "type": "mysql",
  "host": "localhost",
  "port": 3306,
  "username": "your_db_user",
  "password": "your_db_password",
  "database": "your_db_name",
  "synchronize": false,
  "logging": true,
  "entities": ["src/entity/**/*.ts"],
